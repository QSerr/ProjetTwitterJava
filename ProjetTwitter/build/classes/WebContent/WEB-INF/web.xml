<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
         version="2.5" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name>TestTomcat</display-name>
	<servlet> 
		<servlet-name>AddComment</servlet-name> 
		<servlet-class>Servlets.AddComment</servlet-class>
	</servlet>
	<servlet> 
		<servlet-name>GetProfile</servlet-name> 
		<servlet-class>Servlets.GetProfile</servlet-class>
	</servlet>
	<servlet> 
		<servlet-name>ListFriends</servlet-name> 
		<servlet-class>Servlets.ListFriends</servlet-class>
	</servlet>
	<servlet> 
		<servlet-name>ListMessages</servlet-name> 
		<servlet-class>Servlets.ListMessages</servlet-class>
	</servlet>
	<servlet> 
		<servlet-name>SearchMessage</servlet-name> 
		<servlet-class>Servlets.SearchMessage</servlet-class>
	</servlet>
	<servlet> 
		<servlet-name>AddMessages</servlet-name> 
		<servlet-class>Servlets.AddMessage</servlet-class>
	</servlet>
	<servlet> 
		<servlet-name>CreateUser</servlet-name> 
		<servlet-class>Servlets.CreateUser</servlet-class>
	</servlet>
	<servlet> 
		<servlet-name>Login</servlet-name> 
		<servlet-class>Servlets.Login</servlet-class>
	</servlet> 
	<servlet> 
		<servlet-name>Logout</servlet-name> 
		<servlet-class>Servlets.Logout</servlet-class>
	</servlet> 
	<servlet> 
		<servlet-name>AddFriend</servlet-name> 
		<servlet-class>Servlets.AddFriend</servlet-class> 
	</servlet> 
	<servlet> 
		<servlet-name>RemoveFriend</servlet-name> 
		<servlet-class>Servlets.RemoveFriend</servlet-class> 
	</servlet>
	<servlet-mapping> 
		<servlet-name>AddComment</servlet-name> 
		<url-pattern>/Servlet/AddComment</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>AddMessages</servlet-name> 
		<url-pattern>/Servlet/AddMessage</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>CreateUser</servlet-name> 
		<url-pattern>/Servlet/AddUser</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>Login</servlet-name> 
		<url-pattern>/Servlet/Login</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>Logout</servlet-name> 
		<url-pattern>/Servlet/Logout</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>AddFriend</servlet-name> 
		<url-pattern>/Servlet/AddFriend</url-pattern> 
	</servlet-mapping> 
	<servlet-mapping> 
		<servlet-name>RemoveFriend</servlet-name>
		<url-pattern>/Servlet/RemoveFriend</url-pattern> 
	</servlet-mapping> 	
	<servlet-mapping> 
		<servlet-name>SearchMessage</servlet-name> 
		<url-pattern>/Servlet/SearchMessage</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>ListFriends</servlet-name> 
		<url-pattern>/Servlet/ListFriends</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>ListMessages</servlet-name> 
		<url-pattern>/Servlet/ListMessages</url-pattern> 
	</servlet-mapping>
	<servlet-mapping> 
		<servlet-name>GetProfile</servlet-name> 
		<url-pattern>/Servlet/GetProfile</url-pattern> 
	</servlet-mapping>
	<filter>
		<filter-name>CorsFilter</filter-name>
		<filter-class>tools.CORSFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>CorsFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>